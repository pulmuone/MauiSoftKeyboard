<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:vm="clr-namespace:MauiSoftKeyboard.ViewModels"
             xmlns:controls="clr-namespace:MauiSoftKeyboard.Controls"             
             xmlns:effects="clr-namespace:MauiSoftKeyboard.Effects"
             x:Class="MauiSoftKeyboard.MainPage">


    <StackLayout Orientation="Vertical" Spacing="20">

       <Button x:Name="NonMvvm"
               Text="Non-MVVM"
               Clicked="NonMvvm_Clicked"/>

        
       <Button x:Name="Mvvm"
               Text="MVVM (Error)"
               Clicked="Mvvm_Clicked"/>

    </StackLayout>
  


</ContentPage>
